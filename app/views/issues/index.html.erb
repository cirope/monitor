<% if @issues.empty? %>
  <%= render 'filters' %>
  <%= render 'shared/empty_index', message: empty_message %>
<% elsif @alt_partial %>
  <%= render 'index_alt', script: @script, issues: @issues, stats: @stats %>
<% else %>
  <%= render 'filters' %>
  <%= render 'index', script: @script, issues: @issues %>
<% end %>
