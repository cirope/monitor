<div class="card border border-info my-3">
  <div class="card-header bg-info text-white">
    <strong><%= Execution.human_attribute_name 'raw_stderr' %></strong>
  </div>
  <div class="card-body">
    <% exec.pruned_stderr.split("\n").each do |line| %>
      <%= line %><br \>
    <% end %>
  </div>
</div>
